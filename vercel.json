{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/shortener.js"
    },
    {
      "src": "/",
      "dest": "/src/index.html"
    },
    {
      "src": "/sobre",
      "dest": "/src/sobre.html"
    },
    {
      "src": "/qr-code",
      "dest": "/src/qr-code.html"
    },
    {
      "src": "/contato",
      "dest": "/src/contato.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/src/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/src/js/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/src/assets/$1"
    },
    {
      "handle": "filesystem"
    }
  ]
}
