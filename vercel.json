{
    "version": 2,
    "builds": [
      {
        "src": "api/shortener.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/shortener.js"
      },
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/sobre",
        "dest": "/sobre.html"
      },
      {
        "src": "/qr-code",
        "dest": "/qr-code.html"
      },
      {
        "src": "/contato",
        "dest": "/contato.html"
      }
    ]
  }
  