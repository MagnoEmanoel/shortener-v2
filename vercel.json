{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/shortener.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/sobre",
      "dest": "/sobre.html"
    },
    {
      "src": "/qr-code",
      "dest": "/qr-code.html"
    },
    {
      "src": "/contato",
      "dest": "/contato.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/404.html"
    }
  ]
}